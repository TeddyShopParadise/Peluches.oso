@model IEnumerable<TeddyShop.Models.Catalogo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DisponibilidadCatalogo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstiloCatalogo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Compañia_NITNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DisponibilidadCatalogo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstiloCatalogo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Compañia_NITNavigation.NIT)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCatalogo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCatalogo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCatalogo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
