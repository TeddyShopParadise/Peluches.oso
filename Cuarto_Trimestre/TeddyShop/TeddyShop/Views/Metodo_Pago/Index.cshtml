@model IEnumerable<TeddyShop.Models.Metodo_Pago>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreMetodoPago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Factura_IdFacturaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMetodoPago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Factura_IdFacturaNavigation.IdFactura)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NumPago">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NumPago">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NumPago">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
